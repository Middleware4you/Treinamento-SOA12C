<?xml version= '1.0' encoding= 'UTF-8' ?>
<wsdl:definitions
     name="serviceExteno"
     targetNamespace="http://xmlns.oracle.com/HelloWorldApp/HelloWorldProject/serviceExteno"
     xmlns:tns="http://xmlns.oracle.com/HelloWorldApp/HelloWorldProject/serviceExteno"
     xmlns:inp1="http://www.middleware4you.com/pessoa"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema"
     xmlns:soap="http://schemas.xmlsoap.org/wsdl/soap/"
     xmlns:wsdl="http://schemas.xmlsoap.org/wsdl/"
    >
    <wsdl:types>
        <xsd:schema xmlns:xsd="http://www.w3.org/2001/XMLSchema">
            <xsd:import namespace="http://www.middleware4you.com/pessoa" schemaLocation="../Schemas/Pessoa.xsd"/>
        </xsd:schema>
    </wsdl:types>
    <wsdl:message name="requestMessage">
        <wsdl:part name="request" element="inp1:ListarPessoaRequest"/>
    </wsdl:message>
    <wsdl:message name="replyMessage">
        <wsdl:part name="response" element="inp1:ListarPessoasResposta"/>
    </wsdl:message>
    <wsdl:portType name="execute_ptt">
        <wsdl:operation name="listaClienteOperation">
            <wsdl:input message="tns:requestMessage"/>
            <wsdl:output message="tns:replyMessage"/>
        </wsdl:operation>
    </wsdl:portType>
    <wsdl:binding name="execute_bind" type="tns:execute_ptt">
        <soap:binding transport="http://schemas.xmlsoap.org/soap/http"/>
        <wsdl:operation name="listaClienteOperation">
            <soap:operation style="document" soapAction="listaClienteOperation"/>
            <wsdl:input>
                <soap:body use="literal" namespace="http://xmlns.oracle.com/HelloWorldApp/HelloWorldProject/serviceExteno"/>
            </wsdl:input>
            <wsdl:output>
                <soap:body use="literal" namespace="http://xmlns.oracle.com/HelloWorldApp/HelloWorldProject/serviceExteno"/>
            </wsdl:output>
        </wsdl:operation>
    </wsdl:binding>
</wsdl:definitions>
